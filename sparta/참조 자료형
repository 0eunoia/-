public class Main {
    public static void main(String[] args) {

        // 참조 자료형
        
        String sparta = "sparta !!";
        System.out.println(sparta);

        int[] intArray = new int[] {1,2,3,4,5}; // int 배열을 선언과 동시에 초기화
        System.out.println(Arrays.toString(intArray));
      }
}
